{
    "name": "Tweet Buff",
    "version": "1.0",
    "description": "Tweet Buff",
    "background_page": "background.html",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title" : "Tweet Buff",
        "default_popup": "popup.html"
    },
    "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["stylesheets/application.css"]
    }
  ],
   
    "permissions": [
    "tabs",
    "http://*/*"
    ]
    
}